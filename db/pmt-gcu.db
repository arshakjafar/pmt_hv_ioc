record(calc, "$(prefix):u0:hvTripDone")
{
	field(INPA, "$(prefix):u0:hvTripStatus.VAL NPP NMS")
	field(INPB, "85")
    field(INPC, "149")
	field(CALC, "( (A==B)||(A==C))?1:0")
	#field(SCAN, "Passive")
    field(SCAN, "1 second")
	field(SLNK, "$(prefix):u0:hvTripStatus")
}
