record(ai, "$(prefix):gcuTemperature")
{
	field(DESC, "Analog input")
	field(SCAN, "1 second")
	field(DTYP, "PmtAI")
	field(INP, "@$(configFilePath);$(devName);sysmon.temp")
	field(EGUF, "3000")
	field(EGU, "Deg")
	field(PREC, "3")
	field(HOPR, "3000")
	field(LOPR, "0")
	field(HIHI, "70")
	field(HIGH, "")
	field(LOW, "")
	field(LOLO, "25")
	field(HHSV, "MAJOR")
	field(HSV, "NO_ALARM")
	field(LSV, "NO_ALARM")
	field(LLSV, "MINOR")
}

